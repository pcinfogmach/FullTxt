<Window x:Class="TextSeeker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:ftxt="clr-namespace:FullText;assembly=FullText"
        xmlns:local="clr-namespace:TextSeeker"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.DataContext>
        <local:ViewModel x:Name="viewModel" />
    </Window.DataContext>
    <Grid>
        <!--<TextBlock Style="{DynamicResource TextBlockStyle1}" Text="aaaa"/>-->
        <Button Content="Start Process" Width="100" Height="30" VerticalAlignment="Center" HorizontalAlignment="Center" Click="StartProcess_Click"/>
    </Grid>
</Window>
